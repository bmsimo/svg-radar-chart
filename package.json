{
	"name": "svg-radar-chart",
	"description": "A reusable radar chart in SVG.",
	"version": "0.1.1",
	"main": "index.js",
	"files": [
		"index.js",
		"smoothing.js"
	],
	"keywords": [
		"svg",
		"radar",
		"chart",
		"dataviz",
		"visualization"
	],
	"author": "Jannis R <mail@jannisr.de>",
	"homepage": "http://derhuerst.github.io/svg-radar-chart/",
	"repository": "derhuerst/svg-radar-chart",
	"bugs": "https://github.com/derhuerst/svg-radar-chart/issues",
	"license": "ISC",
	"engines": {
		"node": ">=6"
	},
	"dependencies": {
		"d3-shape": "^1.0.3",
		"virtual-dom": "^2.1.1"
	},
	"devDependencies": {
		"babel-preset-env": "^1.6.0",
		"babelify": "^7.3",
		"browserify": "^14.1.0",
		"safe-eval": "^0.3",
		"uglify-es": "^3.1.1",
		"virtual-dom-stringify": "^3.0.1"
	},
	"scripts": {
		"bundle": "browserify -g [ babelify --presets env ] docs/index.js > docs/bundle.js",
		"minify": "uglifyjs -mc --screw-ie8 -- docs/bundle.js > docs/bundle.min.js",
		"build": "npm run bundle && npm run minify",
		"dev": "browserify docs/index.js > docs/bundle.min.js"
	}
}
